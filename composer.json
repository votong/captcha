{
	"name": "votong/captcha",
	"type": "package",
	"description": "Laravel 5 Captcha Package",
	"keywords": ["laravel5 Captcha", "laravel5 Security", "Captcha"],
	"homepage": "https://github.com/votong/captcha",
	"license": "MIT",
	"authors": [
		{
			"name": "Muharrem ERÄ°N",
			"email": "votong175@gmail.com",
			"homepage": "https://github.com/votong",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.4",
		"ext-gd": "*",
		"illuminate/config": "~5.0",
		"illuminate/filesystem": "~5.0",
		"illuminate/support": "~5.0",
		"illuminate/hashing": "~5.0",
		"intervention/image": "~2.2"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"mockery/mockery": "dev-master"
	},
	"autoload": {
		"psr-4": {
			"VoTong\\Captcha\\": "src/"
		},
		"files": [
			"src/helpers.php"
		]
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		],
		"psr-4": {
			"VoTong\\Test\\": "tests/"
		}
	},
	"minimum-stability": "stable"
}
